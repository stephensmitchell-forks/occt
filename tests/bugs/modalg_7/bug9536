puts "REQUIRED OCC9536 ALL: Faulty shapes in variables faulty_1"
puts "TODO OCC9536 ALL: Error: Regression after perform fixshape"
puts "======="
puts "OCC9536"
puts "======="
puts ""
###################################
# Regression after perform fixshape
###################################

restore [locate_data_file bug9536_badfacefixsh.brep] a
checkshape a

regexp {Tolerance MAX=([0-9\-+.eE]+)} [tolerance a] dummy tolerance_1

fixshape r a 1.e-3
checkshape r

regexp {Tolerance MAX=([0-9\-+.eE]+)} [tolerance r] dummy tolerance_2

if { [expr {$tolerance_2 - $tolerance_1}  > $tolerance_1 ]} {
   puts "Error: Regression after perform fixshape"
}
